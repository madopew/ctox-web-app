@model CtoxWebApp.Models.UserRegister
@{
    Layout = "_Auth";
    ViewData["title"] = "Register";
}

@using (Html.BeginForm("Register", "Auth", FormMethod.Post))
{
    <h1>Sign up to CTOX</h1>
    @Html.EditorForModel()
    <input type="submit" class="filled" value="Register"/>
    <span class="different-page">or <a href="/Auth/Login">Login now</a></span>  
}